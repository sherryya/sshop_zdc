<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tmount.db.chatroom.dao.ChatroomMapper" >
  <resultMap id="BaseResultMap_roomid" type="com.tmount.db.chatroom.dto.TZdcChatroomid" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="chatroom_id" property="chatroomId" jdbcType="BIGINT" />
  </resultMap>
  <sql id="Base_Column_List_roomID" >
    id,chatroom_id
  </sql>
  <update id="update_chatroomid" parameterType="com.tmount.db.chatroom.dto.TZdcChatroomid">
    update t_zdc_chatroomid
    set  chatroom_id = #{chatroomId,jdbcType=BIGINT}
    where id = #{id,jdbcType=BIGINT}
  </update>
  <select id="select_chatroomid" resultMap="BaseResultMap_roomid">
    select <include refid="Base_Column_List_roomID" />
      from t_zdc_chatroomid
    </select>
</mapper>
