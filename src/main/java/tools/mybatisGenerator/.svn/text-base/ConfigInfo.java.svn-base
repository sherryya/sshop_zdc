package tools.mybatisGenerator;

import java.sql.Connection;
import java.util.List;

public class ConfigInfo {
	private Connection conn;
	
	private String modelTargetProject;
	private String modelTableTargetPackage;
	private String modelJointableTargetProject;

	private String mappingTargetProject;
	private String mappingTableTargetPackage;
	private String mappingJointableTargetProject;

	private String daoTargetProject;
	private String daoTableTargetPackage;
	private String daoJointableTargetProject;
	
	private List<String> sqlMaps;
	
	public Connection getConn() {
		return conn;
	}
	public void setConn(Connection conn) {
		this.conn = conn;
	}
	public String getModelTargetProject() {
		return modelTargetProject;
	}
	public void setModelTargetProject(String modelTargetProject) {
		this.modelTargetProject = modelTargetProject.replace('\\', '/');
	}
	public String getModelTableTargetPackage() {
		return modelTableTargetPackage;
	}
	public void setModelTableTargetPackage(String modelTableTargetPackage) {
		this.modelTableTargetPackage = modelTableTargetPackage;
	}
	public String getModelJointableTargetProject() {
		return modelJointableTargetProject;
	}
	public void setModelJointableTargetProject(String modelJointableTargetProject) {
		this.modelJointableTargetProject = modelJointableTargetProject;
	}
	public String getMappingTargetProject() {
		return mappingTargetProject;
	}
	public void setMappingTargetProject(String mappingTargetProject) {
		this.mappingTargetProject = mappingTargetProject.replace('\\', '/');
	}
	public String getMappingTableTargetPackage() {
		return mappingTableTargetPackage;
	}
	public void setMappingTableTargetPackage(String mappingTableTargetPackage) {
		this.mappingTableTargetPackage = mappingTableTargetPackage;
	}
	public String getMappingJointableTargetProject() {
		return mappingJointableTargetProject;
	}
	public void setMappingJointableTargetProject(
			String mappingJointableTargetProject) {
		this.mappingJointableTargetProject = mappingJointableTargetProject;
	}
	public String getDaoTargetProject() {
		return daoTargetProject;
	}
	public void setDaoTargetProject(String daoTargetProject) {
		this.daoTargetProject = daoTargetProject.replace('\\', '/');
	}
	public String getDaoTableTargetPackage() {
		return daoTableTargetPackage;
	}
	public void setDaoTableTargetPackage(String daoTableTargetPackage) {
		this.daoTableTargetPackage = daoTableTargetPackage;
	}
	public String getDaoJointableTargetProject() {
		return daoJointableTargetProject;
	}
	public void setDaoJointableTargetProject(String daoJointableTargetProject) {
		this.daoJointableTargetProject = daoJointableTargetProject;
	}
	public List<String> getSqlMaps() {
		return sqlMaps;
	}
	public void setSqlMaps(List<String> sqlMaps) {
		this.sqlMaps = sqlMaps;
	}
}
