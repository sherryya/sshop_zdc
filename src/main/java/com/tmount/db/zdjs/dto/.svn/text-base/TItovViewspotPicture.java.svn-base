package com.tmount.db.zdjs.dto;

public class TItovViewspotPicture {
	private Long id;

	private String lonlatId;

	private String picId;

	private String picPathName;

	private String picIntroduction;

	private String picNote;

	private String picName;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getLonlatId() {
		return lonlatId;
	}

	public void setLonlatId(String lonlatId) {
		this.lonlatId = lonlatId;
	}

	public String getPicId() {
		return picId;
	}

	public void setPicId(String picId) {
		this.picId = picId;
	}

	public String getPicPathName() {
		return picPathName;
	}

	public void setPicPathName(String picPathName) {
		this.picPathName = picPathName;
	}

	public String getPicIntroduction() {
		return picIntroduction;
	}

	public void setPicIntroduction(String picIntroduction) {
		this.picIntroduction = picIntroduction;
	}

	public String getPicNote() {
		return picNote;
	}

	public void setPicNote(String picNote) {
		this.picNote = picNote;
	}

	public String getPicName() {
		return picName;
	}

	public void setPicName(String picName) {
		this.picName = picName;
	}

}
