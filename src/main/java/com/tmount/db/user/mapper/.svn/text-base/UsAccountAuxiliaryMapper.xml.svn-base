<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tmount.db.user.dao.UsAccountAuxiliaryMapper">
	<resultMap id="BaseResultMap" type="com.tmount.db.user.dto.UsAccountAuxiliary">
		<id column="aa_id" property="aa_id" jdbcType="INTEGER" />
		<result column="account_id" property="account_id" jdbcType="VARCHAR" />
		<result column="account_name" property="account_name" jdbcType="VARCHAR" />
		<result column="account_password" property="account_password"
			jdbcType="VARCHAR" />
		<result column="account_role_id" property="account_role_id"
			jdbcType="INTEGER" />
		<result column="account_create_date" property="account_create_date"
			jdbcType="TIMESTAMP" />
	</resultMap>
	<sql id="Base_Column_List">
		aa_id,account_id,account_name,account_password,account_role_id,account_create_date
	</sql>
	<select id="getUsAccountAuxiliary" resultMap="BaseResultMap"
		parameterType="java.lang.String">
		select
		<include refid="Base_Column_List" />
		from t_itov_account_auxiliary
		where account_name = #{account_name,jdbcType=BIGINT}
	</select>
</mapper>