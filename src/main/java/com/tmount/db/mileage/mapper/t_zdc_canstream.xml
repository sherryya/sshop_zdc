	<!-- 车门(4) 车窗(4) 车胎压(4) -->
	<select id="selectCarDoorState" resultMap="DoorResultMap"
		parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		select
		<include refid="Door_Column_List" />
		from t_zdc_canstream where
		DeviceUID=#{deviceuid,jdbcType=VARCHAR}
		order by Upload_Time asc
	</select>
	<select id="selectCarWindowState" resultMap="WindowResultMap"
		parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		select
		<include refid="Window_Column_List" />
		from t_zdc_canstream where DeviceUID=#{deviceuid,jdbcType=VARCHAR}
		order by Upload_Time asc
	</select>
