		from t_zdc_canstream where
		DeviceUID=#{deviceuid,jdbcType=VARCHAR}
		order by Upload_Time asc
	</select>

	<insert id="insert" parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		insert into t_zdc_canstream (
		deviceuid,ignition,reversing,brake,light,left_before_door,rigth_before_door,left_after_door,right_after_door,tail_box_door,befort_cover,steering_wheel_angle,cleaning_liquid,safety_belt,engine_speed,instantaneous_velocity,speed,battery_voltage,outside_temperature,mileage,residual_oil_consumption,residual_oil_consumption_state,battery_voltage_state,upload_time
		)
		values (
		#{deviceuid,jdbcType=VARCHAR},#{ignition,jdbcType=INTEGER},
		#{reversing,jdbcType=INTEGER},#{brake,jdbcType=INTEGER},#{light,jdbcType=INTEGER},
		#{leftBeforeDoor,jdbcType=INTEGER},#{rigthBeforeDoor,jdbcType=INTEGER},#{leftAfterDoor,jdbcType=INTEGER},
		#{rightAfterDoor,jdbcType=INTEGER},#{tailBoxDoor,jdbcType=INTEGER},#{befortCover,jdbcType=INTEGER},
		#{steeringWheelAngle,jdbcType=DECIMAL},#{cleaningLiquid,jdbcType=INTEGER},#{safetyBelt,jdbcType=INTEGER},
		#{engineSpeed,jdbcType=DECIMAL},#{instantaneousVelocity,jdbcType=DECIMAL},#{speed,jdbcType=DECIMAL},
		#{batteryVoltage,jdbcType=DECIMAL},#{outsideTemperature,jdbcType=DECIMAL},#{mileage,jdbcType=DECIMAL},
		#{residualOilConsumption,jdbcType=DECIMAL},#{residualOilConsumptionState,jdbcType=INTEGER},#{batteryVoltageState,jdbcType=INTEGER},
		#{uploadTime,jdbcType=TIMESTAMP}
		)
	</insert>


	<select id="selectByPrimaryKey" resultMap="BaseResultMap"
		parameterType="java.lang.Long">
		select
		<include refid="Base_Column_List" />
		from t_zdc_canstream
		where id = #{id,jdbcType=BIGINT}
	</select>
	<select id="selectAll" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from t_zdc_canstream
	</select>


	<select id="selectForID1" resultMap="BaseResultMap1"
		parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		select id,deviceuid from t_zdc_canstream where
		DeviceUID=#{deviceuid,jdbcType=VARCHAR} and Engine_Speed=0 and
		Mileage_Finish=0 order by Upload_Time asc limit 1
	</select>

	<select id="selectForID2" resultMap="BaseResultMap1"
		parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		select id,deviceuid from t_zdc_canstream where
		DeviceUID=#{deviceuid,jdbcType=VARCHAR} and Engine_Speed=0 and
		Mileage_Finish=0 and id>#{id,jdbcType=BIGINT} order by Upload_Time asc
		limit 1
	</select>

	<select id="selectForID3" resultMap="BaseResultMap"
		parameterType="com.tmount.db.mileage.dto.ZdcCanstream">
		select * from t_zdc_canstream where 
   <![CDATA[ id>=#{id,jdbcType=BIGINT} and id<#{id1,jdbcType=BIGINT} ]]>
		and deviceuid=#{deviceuid,jdbcType=VARCHAR} order by Upload_Time asc
	</select>

	<update id="updateMileFlag" parameterType="java.lang.Long">
		update t_zdc_canstream
		set Mileage_Finish = 1 where id = #{id}
	</update>
	<!-- 查询最新的can盒的数据 -->
	<select id="selectCanNew" resultMap="BaseResultMap"
		parameterType="java.lang.String">
		select
		<include refid="Base_Column_List" />
		from t_zdc_canstream where deviceuid = #{deviceuid} AND  IFNULL(Mileage,0) >0 AND IFNULL(Residual_Oil_Consumption,0)>0 order by
		Upload_Time desc limit 1
	</select>
