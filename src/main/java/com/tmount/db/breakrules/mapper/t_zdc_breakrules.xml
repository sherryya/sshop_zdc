  <insert id="insert" parameterType="com.tmount.db.breakrules.dto.TZdcBreakrules">
    insert into t_zdc_breakrules (
       province,city,hphm,date,area,act,code,fen,money,handled,is_push,is_lookup)
    values (
       #{province,jdbcType=VARCHAR},#{city,jdbcType=VARCHAR},
        #{hphm,jdbcType=VARCHAR},#{date,jdbcType=VARCHAR},#{area,jdbcType=VARCHAR},
        #{act,jdbcType=VARCHAR},#{code,jdbcType=VARCHAR},#{fen,jdbcType=VARCHAR},
        #{money,jdbcType=VARCHAR},#{handled,jdbcType=VARCHAR},#{isPush,jdbcType=VARCHAR},
        #{isLookup,jdbcType=VARCHAR}
    )
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.tmount.db.breakrules.dto.TZdcBreakrules">
    update t_zdc_breakrules
    <set>

      <if test="province != null">
        province = #{province,jdbcType=VARCHAR},
      </if>
      <if test="city != null">
        city = #{city,jdbcType=VARCHAR},
      </if>
      <if test="hphm != null">
        hphm = #{hphm,jdbcType=VARCHAR},
      </if>
      <if test="date != null">
        date = #{date,jdbcType=VARCHAR},
      </if>
      <if test="area != null">
        area = #{area,jdbcType=VARCHAR},
      </if>
      <if test="act != null">
        act = #{act,jdbcType=VARCHAR},
      </if>
      <if test="code != null">
        code = #{code,jdbcType=VARCHAR},
      </if>
      <if test="fen != null">
        fen = #{fen,jdbcType=VARCHAR},
      </if>
      <if test="money != null">
        money = #{money,jdbcType=VARCHAR},
      </if>
      <if test="handled != null">
        handled = #{handled,jdbcType=VARCHAR},
      </if>
      <if test="isPush != null">
        is_push = #{isPush,jdbcType=VARCHAR},
      </if>
      <if test="isLookup != null">
        is_lookup = #{isLookup,jdbcType=VARCHAR}
      </if>
    </set>
    where id = #{id,jdbcType=INTEGER}
  </update>
  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete from t_zdc_breakrules
     where id = #{id,jdbcType=INTEGER}
  </delete>
  <!-- 得到违章驾驶记录 -->
  <select id="getBreakrulesList" resultMap="BaseResultMap" parameterType="java.lang.Integer">
    SELECT a.*,b.car_driving_license_date,b.car_color  FROM `t_zdc_breakrules`  a INNER JOIN t_itov_car b ON a.`hphm`=b.`car_plate_number`
    WHERE b.`account_id`=#{account_id,jdbcType=INTEGER} ORDER BY DATE DESC
    <!-- 可以生成序号，但是 不能按时间排序 -->
<!--     SELECT  (@rowNO := @rowNo+1) AS rowno, a.* FROM (SELECT @rowNO :=0) b ,`t_zdc_breakrules`  a ,t_itov_car b
WHERE a.`hphm`=b.`car_plate_number`  AND b.`account_id`=726  ORDER BY DATE DESC  -->
  </select>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
    select 
    <include refid="Base_Column_List" />
      from t_zdc_breakrules
     where id = #{id,jdbcType=INTEGER}  ORDER BY DATE DESC
  </select>
  <select id="selectAll" resultMap="BaseResultMap"  parameterType="java.lang.String">
    select <include refid="Base_Column_List" />
      from t_zdc_breakrules   ORDER BY DATE DESC
    </select>
   <select id="selectByHphm" resultMap="BaseResultMap"  parameterType="java.lang.String">
    select <include refid="Base_Column_List" />
      from t_zdc_breakrules  t WHERE t.`hphm`=#{hphm}   ORDER BY DATE DESC
    </select>
    <select id="isExistBreakRule" resultType="INTEGER"  parameterType="com.tmount.db.breakrules.dto.TZdcBreakrules">
       SELECT COUNT(*) FROM `t_zdc_breakrules` t WHERE t.`hphm`=#{hphm} AND t.`date`=#{date}
    </select>
</mapper>
