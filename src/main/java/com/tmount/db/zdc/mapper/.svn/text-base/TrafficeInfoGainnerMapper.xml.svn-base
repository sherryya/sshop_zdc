<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tmount.db.zdc.dao.TrafficInfoGainnerMapper">
	<resultMap id="BaseResultMap" type="com.tmount.db.zdc.dto.TrafficRollSub">
		<id column="id" property="id" jdbcType="BIGINT" />
		<result column="region" property="region" jdbcType="VARCHAR" />
		<result column="type" property="type" jdbcType="VARCHAR" />
		<result column="direction" property="direction" jdbcType="VARCHAR" />
		<result column="detail" property="detail" jdbcType="VARCHAR" />
		<result column="time" property="time" jdbcType="VARCHAR" />
		<result column="pointid" property="pointid" jdbcType="VARCHAR" />
	</resultMap>

	<sql id="Base_Column_List">
		id,region,type,direction,detail,time,pointid
	</sql>

	<select id="selectForTraffic" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from t_zdc_traffic limit 100
		<!-- from t_zdc_news limit 100 -->
	</select>

	<!-- 根据ID查询单条数据 -->
	<select id="selectForTrafficbyID" resultMap="BaseResultMap"
		parameterType="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		FROM t_zdc_traffic WHERE id=#{id,jdbcType=BIGINT} order by time desc
		limit 1
	</select>


</mapper>
